<!-- @format -->

<!DOCTYPE html>
<html lang="en">
	<head>
		<%- include('./partials/head') %>
		<title>Detalle de Producto<%=product.name %> </title>
	</head>
	<body>
		<header class="background_amarillo">
			<%- include('partials/header') %>
		</header>
		<main id="main-content">
			<h3 class="style-h3"><%= product.name%></h3>
			<div class="product-wrapper">
			 <article class="product">
				<img
					class="imgs"
					src="<%=product.imagen%>"
					alt="casco"
					width="100%"
				/>
				<div class="product-content text-align">
					<h4 class="style-h4"><%= product.Proveedor%></h4>
					<p class="precios"><%=product.Precio%></p>
					<p class="descripciones"><%= product.Producto%></p>
			 	</div>
			 </article>
			</div>
		</main>
		<%- include('./partials/footer') %>
	</body>
</html>
