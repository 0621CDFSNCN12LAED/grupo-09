<!-- @format -->

<!DOCTYPE html>
<html lang="en">
	<head>
		<%- include('./partials/head') %>
		<title>Detalle de Producto<%=productoUnico.producto %></title>
	</head>
	<body>
		<header class="background_amarillo">
			<%- include('partials/header') %>
		</header>
		<main id="main-content">
			<h3 class="style-h3"><%= productoUnico.producto%></h3>
			<div class="product-wrapper">
				<article class="product">
					<img
						class="imgs"
						src="/imgProductos/<%=productoUnico.imagen%>"
						alt="Imagen-Producto"
						width="100%"
					/>
					<div class="product-content text-align">
						<h4 class="style-h4"><%= productoUnico.proveedor%></h4>
						<p class="precios"><%=productoUnico.precio%></p>
						<p class="descripciones">
							<%= productoUnico.producto%>
						</p>
					</div>
				</article>
			</div>
		</main>
		<%- include('./partials/footer') %>
	</body>
</html>
